head
  title Jobly

body
  +head
  +layout

template(name="jobs")
  each job
    tr
      td {{company}}
      td {{title}}
      td {{location}}

template(name="layout")
  .jobs
    table.table
      thead
        tr
          th Company
          th Title
          th Location
      tbody
        +jobs

template(name="head")
  nav.navbar
    a.navbar-brand WNP
